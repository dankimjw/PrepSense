    "recipe_image":"https:\/\/img.spoonacular.com\/recipes\/2004-312x231.jpg",
    "recipe_data":{
      "id":2004,
      "diets":[
        "vegetarian",
        "gluten free"
      ],
      "image":"https:\/\/img.spoonacular.com\/recipes\/2004-312x231.jpg",
      "title":"Paneer & Spinach Coconut Curry on Cauliflower Rice",
      "cuisines":[
        "Indian"
      ],
      "servings":4,
      "dishTypes":[
        "dinner",
        "main course"
      ],
      "ingredients":[
        "8 oz paneer cheese, cubed",
        "4 cups fresh spinach",
        "1 can (14oz) coconut milk",
        "1 large onion, diced",
        "3 cloves garlic, minced",
        "1 inch ginger, minced",
        "2 tsp curry powder",
        "1 tsp turmeric",
        "1 tsp garam masala",
        "1 head cauliflower, riced",
        "2 tbsp coconut oil",
        "Salt to taste"
      ],
      "instructions":[
        "Heat coconut oil in large pan, saut\u00e9 onion until soft.",
        "Add garlic, ginger, and spices, cook 1 minute.",
        "Add coconut milk and simmer 5 minutes.",
        "Add paneer and spinach, cook until spinach wilts.",
        "Season with salt and simmer 10 minutes.",
        "Steam cauliflower rice for 5 minutes.",
        "Serve curry over cauliflower rice."
      ],
      "readyInMinutes":35
    },
